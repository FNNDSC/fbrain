@Article{ Rousseau2006,
	author = "Francois Rousseau and Orit A Glenn and Bistra Iordanova and Claudia Rodriguez-Carranza and Daniel B Vigneron and James A Barkovich and Colin Studholme",
	title = "Registration-based approach for reconstruction of high-resolution in utero fetal {MR} brain images.",
